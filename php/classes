  *

santa
  -> id
  geo
    -> latitude
    -> longitude
    -> radius
  contactinfo
    -> fname
    -> lname
    -> email
    -> phone
  santainfo
    -> price
    -> rating
    -> picture
    -> age
    -> description
    -> finnish
    -> swedish
    -> english
    -> russian
